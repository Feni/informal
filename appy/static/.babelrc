{
    "presets": [
        "@babel/preset-react",
        ["@babel/preset-env",
        {
            "useBuiltIns": "usage",
            "corejs": 3
        }]
    ],
    "plugins": [
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
        ["@babel/plugin-proposal-class-properties"]
    ]
}
